<h2>Sign Up</h2>

<form class="form signup-form">
    <div class="form-group">
        <label for="user-name">First Name</label>
        <input class="form-input" type="text" id="user-name" />
    </div>
    <div class="form-group">
        <label for="user-last-name">Last Name</label>
        <input class="form-input" type="text" id="user-last-name" />
    </div>
  <div class="form-group">
    <label for="email-signup">Email:</label>
    <input class="form-input" type="text" id="email-signup" />
  </div>
  <div class="form-group">
    <label for="password-signup">Password:</label>
    <input class="form-input" type="password" id="confirm-password-signup" />
  </div>
  <div class="form-group">
    <label for="confirm-password-signup">Confirm Password:</label>
    <input class="form-input" type="password" id="confirm-password-signup" />
  </div>
  <div class="form-group">
    <button class="btn btn-primary" type="submit">Signup</button>
  </div>
</form>

<script src="/js/signup.js"></script>
